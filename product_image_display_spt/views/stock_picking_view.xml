<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="inherit_stock_picking_form_view" model="ir.ui.view">
        <field name="name">inherit.stock.picking.form.view</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
            <xpath expr="//sheet/notebook/page/field[@name='move_ids_without_package']/tree/field[@name='product_id']" position="before">
                <!-- <field name="image" widget="image" height="10px" width="25px"/> -->
                <field name="image" widget="image" style="height:10px;width:25px;"/>
            </xpath>
        </field>
    </record>
</odoo>